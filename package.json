{
    "scripts": {
        "default": "vue build --host 0.0.0.0 --config config/base.vue-build.js App.vue",
        "single-umd": "vue build --lib --prod --config config/single-umd.vue-build.js",
        "all-umd": "webpack --config config/umd.webpack.js",
        "play": "vbuild --host 0.0.0.0 -c config/play.config.js -d",
        "build:play": "vbuild -c config/play.config.js"
    },
    "dependencies": {
        "vue-cli": "^2.8.1",
        "webpack": "^2.2.0"
    },
    "devDependencies": {
        "raw-loader": "^0.5.1",
        "vbuild": "^6.24.14",
        "vue-play": "^3.1.3"
    }
}
